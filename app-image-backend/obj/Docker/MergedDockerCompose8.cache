name: dockercompose10736691313018972006
services:
  app-image-backend:
    build:
      context: C:\Users\Rafik\Documents\Project-Angular-Onyx\app-image-backend
      dockerfile: app-image-backend/Dockerfile
    depends_on:
      mysql_db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      DATABASE_HOST: mysql_db
      DATABASE_NAME: Sportify
      DATABASE_PASSWORD: mypassword
      DATABASE_PORT: "3306"
      DATABASE_USER: myuser
    image: appimagebackend
    networks:
      default: null
    ports:
      - mode: ingress
        target: 443
        published: "4015"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Rafik\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Rafik\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  mysql_db:
    command:
      - --default-authentication-plugin=mysql_native_password
    environment:
      MYSQL_DATABASE: Sportify
      MYSQL_PASSWORD: mypassword
      MYSQL_ROOT_PASSWORD: root
      MYSQL_USER: myuser
    image: mysql:latest
    networks:
      default: null
  phpmyadmin:
    environment:
      PMA_ARBITRARY: "1"
    image: phpmyadmin:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "1200"
        protocol: tcp
networks:
  default:
    name: dockercompose10736691313018972006_default